public class Human {
    private String name;
    private String alibi;
    private String backstory;


    public Human(){
        
    }
    
    public Human(String name, String alibi, String backstory) {
       this.name=name;
        this.alibi = alibi;
        this.backstory = backstory;
      
    
    }

    // getters/setter
    public String getName(){
        return name;
    }
    public String getAlibi() {
        return alibi;
    }

    public String getBackstory() {
        return backstory;
    }
    
   
}
